<Project>
	<!--<PropertyGroup Condition=" '$(TargetFramework)' == 'net7.0'">
		<FrameworkVersion>7.0.0</FrameworkVersion>
		<ExtensionsVersion>7.0.0</ExtensionsVersion>
		<EntityFrameworkVersion>7.0.0</EntityFrameworkVersion>
		<EntityFramrworkIdentityVersion>7.0.0</EntityFramrworkIdentityVersion>
	</PropertyGroup>-->

	<PropertyGroup Condition=" '$(TargetFramework)' == 'net8.0'">
		<FrameworkVersion>8.0.0-rc.2.*</FrameworkVersion>
		<AspNetCoreVersion>8.0.0-rc.2.*</AspNetCoreVersion>
		<ExtensionsVersion>8.0.0-rc.2.*</ExtensionsVersion>
		<EntityFrameworkVersion>8.0.0-rc.2.*</EntityFrameworkVersion>
		<EntityFramrworkIdentityVersion>8.0.0-rc.2.*</EntityFramrworkIdentityVersion>
		<IdentityModelVersion>7.0.3</IdentityModelVersion>
	</PropertyGroup>

	<ItemGroup>
		<!--sourcelink-->
		<PackageReference Update="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All"/>

		<!--tests -->
		<PackageReference Update="Microsoft.AspNetCore.TestHost" Version="$(FrameworkVersion)"/>
		<PackageReference Update="Microsoft.NET.Test.Sdk" Version="17.7.2"/>
		<PackageReference Update="Moq" Version="4.20.69"/>
		<PackageReference Update="xunit" Version="2.5.3"/>
		<PackageReference Update="xunit.runner.visualstudio" Version="2.5.3" PrivateAssets="all"/>
		<PackageReference Update="coverlet.collector" Version="6.0.0" PrivateAssets="all"/>
		<PackageReference Update="coverlet.msbuild" Version="6.0.0" PrivateAssets="all"/>

		<!--benchmark-->
		<PackageReference Update="BenchmarkDotNet" Version="0.13.9"/>

		<!--framework-->
		<PackageReference Update="Microsoft.AspNetCore.Authorization" Version="$(FrameworkVersion)"/>
		<PackageReference Update="Microsoft.AspNetCore.Authentication.OpenIdConnect" Version="$(FrameworkVersion)"/>
		<PackageReference Update="Microsoft.AspNetCore.Components" Version="$(FrameworkVersion)"/>
		<PackageReference Update="Microsoft.AspNetCore.Components.Web" Version="$(FrameworkVersion)"/>
		<PackageReference Update="Microsoft.AspNetCore.Components.WebAssembly" Version="$(FrameworkVersion)"/>
		<PackageReference Update="Microsoft.AspNetCore.Components.WebAssembly.Server" Version="$(FrameworkVersion)"/>
		<PackageReference Update="Microsoft.AspNetCore.Components.WebAssembly.Authentication" Version="$(FrameworkVersion)"/>
		<PackageReference Update="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="$(FrameworkVersion)" PrivateAssets="all" />
		<PackageReference Update="Microsoft.AspNetCore.Diagnostics.EntityFrameworkCore" Version="$(FrameworkVersion)"/>
		<PackageReference Update="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="$(FrameworkVersion)"/>

		<PackageReference Update="Microsoft.EntityFrameworkCore" Version="$(EntityFrameworkVersion)"/>
		<PackageReference Update="Microsoft.EntityFrameworkCore.SqlServer" Version="$(EntityFrameworkVersion)"/>
		<PackageReference Update="Microsoft.EntityFrameworkCore.Relational" Version="$(EntityFrameworkVersion)"/>
		<PackageReference Update="Microsoft.EntityFrameworkCore.Tools" Version="$(EntityFrameworkVersion)"/>
		<PackageReference Update="Microsoft.EntityFrameworkCore.InMemory" Version="$(EntityFrameworkVersion)"/>

		<PackageReference Update="Microsoft.Extensions.DependencyInjection" Version="$(ExtensionsVersion)"/>
		<PackageReference Update="Microsoft.Extensions.DependencyInjection.Abstractions" Version="$(ExtensionsVersion)"/>
		<PackageReference Update="Microsoft.Extensions.FileProviders.Abstractions" Version="$(ExtensionsVersion)"/>

		<PackageReference Update="Microsoft.IdentityModel.Abstractions" Version="$(IdentityModelVersion)"/>
		<PackageReference Update="Microsoft.IdentityModel.Tokens" Version="$(IdentityModelVersion)"/>
		<PackageReference Update="Microsoft.IdentityModel.Protocols" Version="$(IdentityModelVersion)"/>
		<PackageReference Update="Microsoft.IdentityModel.Protocols.OpenIdConnect" Version="$(IdentityModelVersion)"/>

		<!--tools-->
		<PackageReference Update="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="$(FrameworkVersion)"/>

		<!--serilog-->
		<PackageReference Update="Serilog" Version="3.0.1"/>
		<PackageReference Update="Serilog.AspNetCore" Version="7.0.0"/>
		<PackageReference Update="Serilog.Sinks.Console" Version="4.1.0"/>

		<!--packages-->
		<PackageReference Update="JetBrains.Annotations" Version="2023.2.0"/>
	</ItemGroup>
</Project>
