<SonarQubeAnalysisProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                             xmlns="http://www.sonarsource.com/msbuild/integration/2015/1">
    <!-- Generic  -->
    <Property Name="sonar.projectKey">wangkanai_wangkanai</Property>
    <Property Name="sonar.organization">wangkanai</Property>
    <Property Name="sonar.host.url">https://sonarcloud.io</Property>
    
    <Property Name="sonar.branch.name">main</Property>
    <Property Name="sonar.project.monorepo.enabled">true</Property>
    <!-- Source   -->
    <Property Name="sonar.source">.</Property>
    <Property name="sonar.exclusions">**/node_modules/**/*, **/lib/**/*, **/BenchmarkDotNet.Artifacts/**/*</Property>
    <!-- Tests    -->
    <!--    <Property Name="sonar.tests"></Property>-->
    <!--    <Property Name="sonar.tests.inclusions"></Property>-->
    <!--    <Property Name="sonar.tests.exclusions"></Property>-->
    <!-- Coverage -->
    <Property Name="sonar.cs.vscoveragexml.reportsPaths">coverage.xml</Property>
    <Property Name="sonar.coverage.exclusions">**/tests/**/*, **/demo/**/*, **/samples/**/*, **/hosts/**/*, **/benchmark/**/*</Property>
    <!-- Duplicate-->
</SonarQubeAnalysisProperties>