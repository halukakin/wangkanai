<Project>
    <PropertyGroup>
        <VersionPrefix>0.1.3</VersionPrefix>

        <Title>Wangkanai Tabler</Title>
        <PackageTags>aspnetcore;blazor;tabler;</PackageTags>
        <Description>This project is blazor wrapper for the Tabler CSS framework</Description>
        
    </PropertyGroup>

    <PropertyGroup Condition="$(MSBuildProjectName.Contains('Demo')) == 'False'">
        <PackageIcon>wangkanai-logo.png</PackageIcon>
        <PackageReadmeFile>README.md</PackageReadmeFile>
    </PropertyGroup>

    <ItemGroup Condition="$(MSBuildProjectName.Contains('Demo')) == 'False'">
        <None Include="..\..\asset\wangkanai-logo.png" Pack="true" PackagePath="\"/>
        <None Include="..\README.md" Pack="true" PackagePath="\"/>
    </ItemGroup>
</Project>